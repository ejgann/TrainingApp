<h1>Add a New Workout</h1>

<%= form_for(@workout) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<br>
<%= f.label :category %>
<%= f.text_field :category %>
<br>
<br>
<%= f.label :duration %>
<%= f.text_field :duration %>
<br>
<br>
<%= f.label :intensity %>
<%= f.text_field :intensity %> On a scale of 1 to 5 (5 = most difficult)
<br>
<br>
<%= f.submit('Add')%>   |   <%= link_to 'Cancel', events_path %>

<% end %>