<h1>My Recorded Trainings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Notes</th>
      <th>Associated Workout</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trainings.each do |training| %>
      <tr>
        <td><li><%= training.date %></td>
        <td><%= training.notes %></td>
        <td><%= link_to training.workout.name, workout_path(@workout) %></td>

      </tr>
    <% end %>
  </tbody>
</table>